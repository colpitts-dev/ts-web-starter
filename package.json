{
  "name": "ts-web-starter",
  "version": "1.0.0",
  "description": "TypeScript express boilerplate",
  "author": "acolpitts <adam@colpitts.dev>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "babel src --out-dir dist",
    "build:watch": "yarn build --watch",
    "dev": "concurrently -n babel,app \"yarn build:watch\" \"nodemon -q dist/index.js\"",
    "start": "cross-env NODE_ENV=production node ./dist/server.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.20"
  }
}
